<main>
  <div class="post-container">
    <% messages.forEach((message) => { %>
      <div class="grid-cell">
        <article class="post">
          <div class="heading">
            <h5 class="title"><%= message.title %></h5>
          </div>
          <p class="content"><%= message.text %></p>
          <p class="username">Anonymous</p>
        </article>
      </div>
    <% }) %>
  </div>
</main>